{
  "presets": ["react", "es2015"],
  "env": {
    "development": {
      "plugins": [
        "transform-class-properties",
        "transform-export-extensions",
        ["react-transform", {
          "transforms": [{
            "transform": "react-transform-hmr",
            "imports": ["react"],
            "locals": ["module"]
          }, {
            "transform": "react-transform-catch-errors",
            "imports": ["react", "redbox-react"]
          }]
        }]
      ]
    }
  }
}
